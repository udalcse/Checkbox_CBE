    <!-- ================================================================
       == Navigation SiteMap Configuration                           ==
       =================================================================  -->
    <siteMap defaultProvider="navMenuSiteMap">
      <providers>
        <!-- Site Map Provider for Top-Level Navigation Menu -->
        <add name="navMenuSiteMap" type="Checkbox.Web.UI.Controls.Menus.SecuredSiteMapProvider" siteMapFile="~/NavMenu.sitemap" securityTrimmingEnabled="true" />
      </providers>
    </siteMap>
  </system.web>
  <!-- ================================================================
     == Compiler Settings                                          ==
     =================================================================  -->
  <!-- ================================================================
     == Web Server Settings Required for AJAX Under IIS7           ==
     =================================================================  -->
  <!--
        The system.webServer section is required for running ASP.NET AJAX under Internet
        Information Services 7.0.  It is not necessary for previous version of IIS, but
        can remain as it should not cause any compatibility issues.
    -->
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <rewrite>
      <!--<rules>
        <rule name="Redirect HTTP to HTTPS">
          <match url="(.*)" />
          <conditions>
            <add input="{HTTPS}" pattern="off" ignoreCase="true" />
          </conditions>
          <action type="Redirect" url="https://{HTTP_HOST}/{R:1}" redirectType="Permanent" />
        </rule>
      </rules>-->
    </rewrite>
    <modules runAllManagedModulesForAllRequests="true">
      <!-- Uncomment the following when configuring Checkbox to use Alternate URLs for Surveys -->
      <remove name="SurveyUrlRewriter" />
      <remove name="BundleModule" />
      <add type="Checkbox.Web.UrlRewriting.SurveyUrlRewriter, Checkbox.Web" name="SurveyUrlRewriter" />
      <add name="BundleModule" type="System.Web.Optimization.BundleModule" />
    </modules>
    <handlers>
      <remove name="WebServiceHandlerFactory-Integrated" />
      <remove name="ScriptHandlerFactory" />
      <remove name="ScriptHandlerFactoryAppServices" />
      <remove name="ScriptResource" />
      <remove name="ChartImageHandler" />
      <add name="ScriptResource" preCondition="integratedMode" verb="GET,HEAD" path="ScriptResource.axd" type="System.Web.Handlers.ScriptResourceHandler, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
      <add name="ChartImageHandler" preCondition="integratedMode" verb="GET,HEAD" path="ChartImg.axd" type="System.Web.UI.DataVisualization.Charting.ChartHttpHandler, System.Web.DataVisualization, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
      <add name="UFrameFileUploadHandler_axd" preCondition="integratedMode" verb="*" path="UFrameFileUploadHandler.axd" type="Checkbox.Web.UI.Controls.UFrameFileUploadHandler" />
    </handlers>
    <!--
        GZip static file content.  Overrides the server default which only compresses static files over 2700 bytes
      -->
    <httpCompression directory="%SystemDrive%\websites\_compressed" minFileSizeForComp="1024">
      <scheme name="gzip" dll="%Windir%\system32\inetsrv\gzip.dll" />
      <staticTypes>
        <add mimeType="text/*" enabled="true" />
        <add mimeType="message/*" enabled="true" />
        <add mimeType="application/javascript" enabled="true" />
        <add mimeType="application/json" enabled="true" />
        <add mimeType="*/*" enabled="false" />
      </staticTypes>
    </httpCompression>
    <urlCompression doStaticCompression="true" doDynamicCompression="true" />
    <httpProtocol>
      <customHeaders>
        <!-- Force the latest IE version, in various cases when it may fall back to IE7 mode
    github.com/rails/rails/commit/123eb25#commitcomment-118920
    Use ChromeFrame if it's installed for a better experience for the poor IE folk -->
        <add name="X-UA-Compatible" value="IE=Edge,chrome=IE8" />
        <remove name="X-Powered-By" />
      </customHeaders>
    </httpProtocol>
    <staticContent>
      <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="00.00:30:00" />
      <remove fileExtension=".eot" />
      <mimeMap fileExtension=".eot" mimeType="application/vnd.ms-fontobject" />
      <remove fileExtension=".svg" />
      <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
      <remove fileExtension=".ttf" />
      <mimeMap fileExtension=".ttf" mimeType="application/x-font-ttf" />
      <remove fileExtension=".woff" />
      <mimeMap fileExtension=".woff" mimeType="application/x-font-woff" />
      <remove fileExtension=".woff2" />
      <mimeMap fileExtension=".woff2" mimeType="application/font-woff2" />
    </staticContent>
    
  </system.webServer>
  <!--  =================================================================
        == WCF Service Configuration for REST Services.                ==
        =================================================================  -->
  <!-- In the default Checkbox configuration, response processing is performed
       on same web server as Checkbox installation, so no WCF calls are necessary.
       In that case, section below can remain commented out.  Other scenarios,
       such as building remote renderers or offloading response processing to a
