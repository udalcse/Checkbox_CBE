  <!-- =================================================================
       == Forms Authentication Configuration                          ==
       =================================================================  -->
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
  -->
  <system.web>
    <!--
            The <authentication> section enables configuration
            of the security authentication mode used by
            ASP.NET to identify an incoming user.
        -->
    <authentication mode="Forms">
      <forms loginUrl="~/Login.aspx" cookieless="UseCookies" />
    </authentication>
    <!-- Deny anonymous access to pages not listed above. -->
    <authorization>
      <deny users="?" />
    </authorization>
    <!-- Uncomment section to use DyanmoDB for session state storage -->
    <!--
    <sessionState timeout="20" mode="Custom" customProvider="DynamoDBSessionStoreProvider">
